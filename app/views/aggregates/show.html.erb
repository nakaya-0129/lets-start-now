<%= render "shared/login" %>
<div class="object-show-box">
    <table class="detail-table">
      <tbody>
        <tr>
          <th class="detail-object">成長した能力</th>
          <td class="detail-value"><%= @aggregate.ability.name %></td>
        </tr>
        <tr>
          <th class="detail-object">スタート</th>
          <td class="detail-value"><%= @aggregate.start_day %></td>
        </tr>
        <tr>
          <th class="detail-object">期間</th>
          <td class="detail-value"><%= @aggregate.period.name %></td>
        </tr>
        <tr>
          <th class="detail-object">達成率</th>
          <td class="detail-value"><%= @aggregate.action_point %></td>
        </tr>
        <tr>
          <th class="detail-object">成長値</th>
          <td class="detail-value"><%= @aggregate.ability_point %></td>
        </tr>
        <tr>
          <th class="detail-object">自分への褒め言葉</th>
          <td class="detail-value"><%= @aggregate.self_compliment %></td>
        </tr>
      </tbody>
    </table>
     <%= link_to '削除', aggregate_path(@aggregate.id), method: :delete, class:'btn-object' %>
    </div>
